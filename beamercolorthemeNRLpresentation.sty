% define some NRL colors taken from the PowerPoint template
\definecolor{NRLgold}{RGB}{250,190,7}% xFABE07
\definecolor{NRLsky}{RGB}{7,89,255}% x0759FF
%\definecolor{NRLblue}{RGB}{0,32,96}% x002060 % corporate blue
\definecolor{NRLblue}{RGB}{23,42,86}% X172A56 % matches right edge of header
\definecolor{NRLnavy}{RGB}{0,18,54}% x001236

% Define various named color schemes.
% facets title page with white text
\def\NRLtitlecolorFacets{
  \def\NRLtitlecolor@kind{facets}
  \NRLfacetstrue
  \NRLlogo{NRL_FullLogo_White}
  \setbeamercolor{titlelike}{fg=white,bg=NRLblue}
  \setbeamercolor{bottombox}{fg=white,bg=NRLnavy}
  \setbeamercolor{author}{fg=NRLgold}
  \setbeamercolor{block title}{fg=NRLsky}
}%

% blue title page with white text
\def\NRLtitlecolorBlue{
  \def\NRLtitlecolor@kind{blue}
  \NRLfacetsfalse
  \NRLlogo{NRL_FullLogo_White}
  \setbeamercolor{titlelike}{fg=white,bg=NRLblue}
  \setbeamercolor{bottombox}{fg=white,bg=NRLnavy}
  \setbeamercolor{author}{fg=NRLgold}
  \setbeamercolor{block title}{fg=NRLsky}
}%

% white title page with black text
\def\NRLtitlecolorWhite{
  \def\NRLtitlecolor@kind{white}
  \NRLfacetsfalse
  \NRLlogo{NRL_FullLogo_Black}
  \setbeamercolor{titlelike}{fg=black,bg=white}
  \setbeamercolor{bottombox}{fg=black,bg=white}
  \setbeamercolor{author}{fg=black}
  \setbeamercolor{block title}{fg=black}
}%

% white slide background with black text
\def\NRLpagecolorWhite{
  \setbeamercolor{normal text}{fg=black,bg=white}\usebeamercolor[fg]{normal text}
  \ifx\NRLtitlecolor@kind\NRLtitlecolor@white%
    \setbeamercolor{block title}{fg=black}
  \else
    \setbeamercolor{block title}{fg=NRLsky}
  \fi%
  \setbeamercolor{frametitle}{parent=titlelike}
}%

% blue slide background with white text
\def\NRLpagecolorBlue{
  \setbeamercolor{normal text}{fg=white,bg=NRLblue}\usebeamercolor[fg]{normal text}
  \setbeamercolor{block title}{fg=NRLgold}
  \setbeamercolor{frametitle}{parent=bottombox}
}%

% navy slide background with white text
\def\NRLpagecolorNavy{
  \setbeamercolor{normal text}{fg=white,bg=NRLnavy}\usebeamercolor[fg]{normal text}
  \setbeamercolor{block title}{fg=NRLgold}
  \setbeamercolor{frametitle}{parent=titlelike}
}%

% load the chosen colorscheme
\ifNRLfacets
  \NRLtitlecolorFacets%
\fi%
\ifx\NRLtitlecolor@kind\NRLtitlecolor@blue%
  \NRLtitlecolorBlue%
\fi%
\ifx\NRLtitlecolor@kind\NRLtitlecolor@white%
  \NRLtitlecolorWhite%
\fi%
\ifx\NRLpagecolor@kind\NRLpagecolor@white%
  \NRLpagecolorWhite%
\fi%
\ifx\NRLpagecolor@kind\NRLpagecolor@blue%
  \NRLpagecolorBlue%
\fi%
\ifx\NRLpagecolor@kind\NRLpagecolor@navy%
  \NRLpagecolorNavy%
\fi%

